{
  "name": "bug-logger",
  "description": "bug-logger",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "dev": "rimraf dist && run-p dev:webpack dev:electron",
    "build": "rimraf dist && cross-env NODE_ENV=\"production\" webpack --progress",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=assets/icon.ico --prune=false --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"BugLogger\"",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on dist/main.js dist/index.html && electronmon ."
  },
  "electronmon": {
    "patterns": [
      "dist/**/*"
    ]
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "dotenv": "^16.0.3",
    "moment": "^2.29.4",
    "mongoose": "^6.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-moment": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "electron": "^22.0.0",
    "electron-packager": "^17.1.1",
    "electronmon": "^2.0.2",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "wait-on": "^7.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
